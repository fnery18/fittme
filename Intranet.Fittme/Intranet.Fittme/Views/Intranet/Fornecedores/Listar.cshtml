@model List<Intranet.Fittme.Models.FornecedorModel>
@{
    ViewBag.Title = "Listar";
}

@section styles{

}
<div class="limiter">
    <div class="container-table100">
        <div class="wrap-table100">
            <div class="table">

                <div class="row header">
                    <div class="cell">
                        Nome
                    </div>
                    <div class="cell">
                        Email
                    </div>
                    <div class="cell">
                        Celular
                    </div>
                </div>

                @foreach (var item in Model)
                {
                    <div class="row">
                        <div class="cell" data-title="Nome">
                            @item.Nome
                        </div>
                        <div class="cell" data-title="Email">
                            @item.Email
                        </div>
                        <div class="cell" data-title="Celular">
                            @item.Celular
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>

@section scripts{
<link href="~/Content/tabela/table.css" rel="stylesheet" />
}
